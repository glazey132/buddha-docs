{
  "name": "electron-react-quick-start",
  "version": "1.0.0",
  "description": "An aesthetically pleasing, collaborative rich-text editor",
  "build": {
    "appId": "com.richtext.8080",
    "productName": "Buddha Docs",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main.js"
    ],
    "mac": {
      "category": "public.app-category.Application"
    },
    "linux": {
      "category": "Office"
    },
    "win": {}
  },
  "main": "main.js",
  "scripts": {
    "devFront": "concurrently -k \"npm run devWebpack\" \"npm run devApp\"",
    "devWebpack": "webpack-dev-server --content-base build/",
    "devApp": "electron main.js --dev",
    "webpack": "webpack",
    "start": "electron main.js",
    "dist": "electron-builder"
  },
  "homepage": "./",
  "repository": "",
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "electron": "~1.6.2",
    "electron-builder": "^20.27.1",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "loglevel": "^1.4.1",
    "nodemon": "^1.11.0",
    "style-loader": "^0.19.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.6.0"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.0",
    "draft-js": "^0.10.4",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "material-ui": "^0.20.2",
    "mongoose": "^4.12.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^15.6.1",
    "react-color": "^2.14.1",
    "react-dom": "^15.6.1",
    "react-materialize": "^2.3.3",
    "react-router-dom": "^4.2.2",
    "socket.io": "^2.1.1",
    "underscore": "^1.9.1"
  }
}
